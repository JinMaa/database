<!-- Transaction Count Visualization Page -->
<div class="container-fluid mt-4">
  <div class="row">
    <div class="col-12">
      <div class="card">
        <div class="card-header">
          <h2>Clock-In Percentage by Height</h2>
          <p class="text-muted">Visualization of clock-in transactions as a percentage of total transactions by block height</p>
        </div>
        <div class="card-body">
          <div id="loading" class="text-center my-5">
            <div class="spinner-border text-primary" role="status">
              <span class="visually-hidden">Loading...</span>
            </div>
            <p class="mt-2">Loading data...</p>
          </div>
          
          <div id="visualization-container" style="display: none;">
            <div id="block-visualization" class="mb-5" style="width: 100%; height: 650px;"></div>
            
            <div class="row mt-4">
              <div class="col-12">
                <div class="card">
                  <div class="card-header">
                    <h4>Data Table</h4>
                  </div>
                  <div class="card-body">
                    <div class="table-responsive">
                      <table id="data-table" class="table table-striped table-hover">
                        <thead>
                          <tr>
                            <th>Block Height</th>
                            <th>Total Transactions</th>
                            <th>Clock-In Transactions</th>
                            <th>Clock-In Percentage</th>
                            <th>Max Clock-In Count</th>
                          </tr>
                        </thead>
                        <tbody></tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

